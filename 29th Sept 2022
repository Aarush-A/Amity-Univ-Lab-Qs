    return 0;
}

/var swap with 3rd var
#include <stdio.h>

int main()
{
    int v1,v2,temp;
    printf("Enter two numbers:");
    scanf("%d%d",&v1,&v2);
    printf("First Var:%d\n",v1);
    printf("Second Var:%d\n",v2);
    printf("Swapping\n");
    temp=v1;
    v1=v2;
    v2=temp;
    printf("First Var:%d\n",v1);
    printf("Second Var:%d\n",v2);
}

/var swap w/o 3rd var
#include <stdio.h>

int main()
{
    int v1,v2;
    printf("Enter two numbers:");
    scanf("%d%d",&v1,&v2);
    printf("First Var:%d\n",v1);
    printf("Second Var:%d\n",v2);
    printf("Swapping\n");
    v1=v1+v2;
    v2=v1-v2;
    v1=v1-v2;
    printf("First Var:%d\n",v1);
    printf("Second Var:%d\n",v2);
}


/celcius to farheneit
#include <stdio.h>

int main()
{
    float cel,far;
    printf("Enter temperature in celcius:");
    scanf("%f",&cel);
    far=((cel)*1.8)+32;
    printf("The Temperature in farheneit is: %f",far);
}



